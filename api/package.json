{
  "name": "azure-function-typescript-v4",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "build:production": "npm run prestart && npm prune --production",
    "watch": "tsc --w",
    "prestart": "npm run build && func extensions install && npm run gen",
    "start:host": "func start",
    "start": "npm-run-all --parallel start:host watch gen:watch",
    "test": "echo \"No tests yet...\"",
    "gen": "graphql-codegen --config codegen.yml",
    "gen:watch": "graphql-codegen --config codegen.yml --watch"
  },
  "devDependencies": {
    "@azure/functions": "^3.0.0",
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typed-document-node": "^2.3.3",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.3",
    "@types/node": "16.x",
    "azure-functions-core-tools": "^4.0.4704",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@azure/cosmos": "^3.16.4",
    "@graphql-tools/graphql-file-loader": "^7.5.2",
    "@graphql-tools/load": "^7.7.4",
    "@graphql-tools/schema": "^9.0.1",
    "apollo-datasource": "^3.3.2",
    "apollo-datasource-cosmosdb": "^0.1.0",
    "apollo-server-azure-functions": "^3.10.1"
  }
}
